<form id="delete-editor" method="post" action="{{ path('app_editor_contact_delete_multiple') }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('editor-delete-multiple-contacts') }}">
    <button class="btn btn-danger" type="submit">
        {% trans %}
            Delete Selected Editor
        {% endtrans %}
    </button>
</form>